<manifest package="com.tokeninc.barcodescanner"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:hardwareAccelerated="true">
        <activity android:name=".TokenCaptureActivity">
            <intent-filter>
                <action android:name="com.tokeninc.barcodescanner.REQUEST_BARCODE"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
